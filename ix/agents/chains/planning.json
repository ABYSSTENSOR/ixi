{
  "input_keys": ["user_input"],
  "class_path": "ix.agents.chains.tool_chooser.ChooseTool",
  "config": {
    "llm": {
      "class_path": "langchain.chat_models.openai.ChatOpenAI",
      "config": {
        "temperature": 0
      }
    },
    "tools": [
      {
        "name": "create",
        "description": "create a new PLAN to fulfill the user request",
        "class_path": "ix.agents.chains.routing.IXSequence",
        "config": {
          "input_variables": ["user_input"],
          "chains": [
            {
              "class_path": "ix.agents.chains.planning.CreatePlan",
              "config": {
                "llm": {
                  "class_path": "langchain.chat_models.openai.ChatOpenAI",
                  "config": {
                    "request_timeout": 240,
                    "temperature": 0.2,
                    "verbose": true
                  }
                },
                "tools": [
                    "ix.commands.google",
                    "ix.commands.filesystem",
                    "ix.commands.execute"
                  ]
              }
            },
            {
              "class_path": "ix.agents.chains.json.ParseJSON"
            },
            {
              "class_path": "ix.agents.chains.planning.SavePlan"
            }
          ]
        }
      }
    ]
  }
}
